// --- Corner stati, and other setup equates.

// for cornersRez

#define off			0b00000000
#define dontBlank		0b00000001
#define bkgblankNow	0b00000010
#define blanknow		0b00000011

// for optionsRez

#define fT2Active		$0001	// bit 0
#define fBlinkingBox	$0002	// bit 1
#define fCapsLockLock	$0004	// bit 2
#define fWarningAlerts	$0008	// bit 3
#define fInstallNDA	$0010	// bit 4
//#define fRandomize		$0020	// bit 5
#define fLetMouseRestore $0040	// bit 6
#define fNoSound		$0080	// bit 7 (v1.0.1b2 - 12/13/92)

#define fWatchBkgBlank	$0000	// bits 8-9: 00 = watchbkgblank  | 11=illegal
#define fWatchDontBlank	$0100	//           01 = watchdontblank |
#define fWatchNormBlank	$0200	//           10 = watchnormblank |

#define fDClickPreview	$0000	// bits 10-11: 00 = dclickpreviewmodule
#define fDClickClose	$0400	//             01 = dclickcloset2
#define fDClickSetup	$0800	//             10 = dclicksetupmodule
#define fDClickToggle	$0C00	//             11 = dclickToggle

#define fTextBkgBlank	$0000	// bits 12-13: 00 = textbkgblank  | 11=illegal
#define fTextGSOSBlank	$1000	//             01 = textgsosblank |
#define fTextDontBlank	$2000	//             10 = textdontblank |

#define fSysBeepsUnblank $4000	// bit 14 (T2 v1.0.1b2 - 12/13/92)
#define fUseIntelliKey	$8000	// bit 15 (T2 v1.0.1b2 - 12/13/92) - end!

// for options2rez (1.0.1b3)

#define fLowMemoryMode	$0001	// bit 0
#define fSHRCorners	$0002	// bit 1
#define fSwapModules	$0004	// bit 2
#define fPassword		$0008	// bit 3
#define fNewModulePath	$0010	// bit 4


