// *===========================================================================
// *== wait (thermometer) window

resource rControlTemplate (WaitThermoCtl) {
		1,			// ID
		{ 19, 12, 28,260},		// rect
		thermometerControl {{
			$0001,			// flag -- horizontal thermometer
			fCtlProcNotPtr+RefIsResource,	// moreFlags
			NIL,			// refCon
			NIL,			// value (position of mercury)
			10			// scale (will be set by T2!)
//			colorTableRef
		}};
};

resource rControlTemplate (WaitWaitStrCtl) {
		2,			// ID
		{  5, 12, 15,293},		// rect
		statTextControl {{
	       	ctlVisible,   // flag
	       	fCtlProcNotPtr+RefIsResource,  // moreFlags
	       	NIL,	 // refCon
			WaitWait_LText		// textRef
		}};
};

// *===========================================================================
// *== setup window

resource rControlTemplate (T2SetupLineCtl) {
       $07FEFFFF,	       // ID
       { 20,0, 21,350},		  // rect
       rectangleControl {{
	       $FF02,		    // flag (%10 = black pattern)
	       fCtlProcNotPtr,	    // moreFlags (required values)
	       NIL	      // refCon
       }};
};

resource rControlTemplate (T2SetupUpdateCtl) {
       $07FEFFFE,	       // ID
		{  4,290, 16,344},		// rect
       simpleButtonControl {{
	       ctlVisible,   // flag
	       fCtlProcNotPtr+fCtlWantsEvents+RefIsResource, // moreFlags
	       NIL,	 // refCon
	       T2SetupUpdateStr,  // titleRef
	       0,	       // colorTableRef
	       {"S","s",$0100,$0100}  // key equivalents
       }};
};

resource rControlTemplate (T2SetupPopupCtl) {
       $07FEFFFD,		    // ID
       {4,4,16,228},		    // rect
       popUpControl {{
	       ctlVisible+fType2PopUp,	   // flag
	       fCtlProcNotPtr+RefIsHandle+fDrawPopDownIcon, // moreFlags
	       NIL,		    // refCon
	       NIL,		    // titleWidth
	       T2SetupModuleMenu,   // menuRef == handle; filled in by T2
	       $200		    // initialValue
       }};
};

resource rControlTemplate (T2SetupTestCtl) {
		$07FEFFFC,		    // ID
		{  4,232, 16,286},		// rect
		simpleButtonControl {{
			NIL,			// flag
			fCtlProcNotPtr+fCtlWantsEvents+RefIsResource, // moreFlags
			NIL,			// refCon
			T2SetupTestStr,	// titleRef
			0,			// colorTableRef
		{"T","t",$0100,$0100}	// key equivalents
		}};
};

resource rMenu (T2SetupModuleMenu, nospecialmemory,nocrossbank) {
       $0001,			    // menuID
       $8000+fAllowCache,	    // menuFlag (title=Rez, items=Pointers)
       T2SetupPopupStr, {		    // menuTitleRef
	       T2CornerMItem,
	       T2OptionsMItem,
		T2Options2MItem,
	       NIL		    // menu item list terminator
       };
};

