};


// --- Configuration resources

resource rT2Setup1 (TimeRez) {	     /* Minutes before blanking * 120 */
       120*3
};

resource rT2Setup1 (SwapTimeRez) {	/* Seconds before swapping */
       15*4
};

resource rT2Setup1 (OptionsRez) {		/* all options in 1 word! v1.0d34 */
	fSysBeepsUnblank+
	fUseIntelliKey+
	fLetMouseRestore+
	fT2Active+
	fCapsLockLock+				// NEW!!!!!!!!
/*	fBlinkingBox+ */
	fWarningAlerts+
	fInstallNDA+
	fWatchBkgBlank+
	fDClickPreview+
	fTextBkgBlank
};	

resource rT2Setup1 (Options2Rez) {
	fSHRCorners
};

resource rT2Setup1 (CornersRez) {		/* all corners!  new v1.0d34 */
	off+				// top left
	bkgblanknow*8+		// bottom left
	blanknow*64+		// top right
	dontblank*512		// bottom right
};

